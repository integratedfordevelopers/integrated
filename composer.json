{
    "name": "integrated/solr-bundle",
    "type": "symfony-bundle",
    "description": "Adds support for Solr configuration and indexing of your documents or entities",
    "keywords": ["Integrated", "Solr"],
    "homepage": "http://www.e-active.nl",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "http://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.4",
        "symfony/filesystem": "~2.6",
        "integrated/library": "~0.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*",
        "phpunit/phpunit": "4.2.*"
    },
    "autoload":{
        "psr-0":{
            "Integrated\\Bundle\\SolrBundle": ""
        }
    },
    "target-dir": "Integrated/Bundle/SolrBundle"
}