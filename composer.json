{
    "name": "integrated/website-bundle",
    "type": "symfony-bundle",
    "description": "Provides a website front-end for content in Integrated",
    "keywords": ["Integrated", "website"],
    "homepage": "http://www.integratedfordevelopers.com",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "http://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "ext-mongo": "~1.2",
        "symfony/symfony": ">=2.4",
        "symfony/framework-bundle": ">=2.2",
        "sensio/framework-extra-bundle": "~2.3",
        "doctrine/mongodb": ">=1.0.0",
        "doctrine/mongodb-odm": "*",
        "doctrine/mongodb-odm-bundle": "~3.0",
        "integrated/content-bundle": "~0.3",
        "integrated/block-bundle": "~0.4",
        "integrated/page-bundle": "~0.3",
        "integrated/theme-bundle": "~0.3",
        "integrated/menu-bundle": "~0.4",
        "integrated/channel-bundle": "~0.3",
        "knplabs/knp-paginator-bundle": ">=2.4",
        "friendsofsymfony/jsrouting-bundle": "1.5.*",
        "twbs/bootstrap": "3.*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*",
        "phpunit/phpunit": "~4.8"
    },
    "suggest": {
        "integrated/channel-bundle": "Manage channel configuration"
    },
    "autoload":{
        "psr-4":{
            "Integrated\\Bundle\\WebsiteBundle\\": ""
        }
    }
}
