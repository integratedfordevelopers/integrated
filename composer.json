{
    "name": "integrated/locking-bundle",
    "type": "symfony-bundle",
    "description": "Integrated Locking Bundle",
    "keywords": ["Integrated", "locking"],
    "homepage": "http://www.e-active.nl",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "http://www.e-active.nl"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
