{
    "name": "integrated/website-bundle",
    "type": "symfony-bundle",
    "description": "Provides a website front-end for content in Integrated",
    "keywords": ["Integrated", "website"],
    "homepage": "http://www.integratedfordevelopers.com",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "http://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "ext-mongo": "~1.2",
        "symfony/symfony": "~2.8 || ~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "doctrine/mongodb": "~1.3",
        "doctrine/mongodb-odm": "~1.1",
        "doctrine/mongodb-odm-bundle": "~3.2",
        "integrated/library": "~0.7",
        "integrated/content-bundle": "~0.7",
        "integrated/block-bundle": "~0.7",
        "integrated/page-bundle": "~0.7",
        "integrated/theme-bundle": "~0.7",
        "integrated/menu-bundle": "~0.7",
        "integrated/channel-bundle": "~0.7",
        "knplabs/knp-paginator-bundle": "~2.5",
        "friendsofsymfony/jsrouting-bundle": "~1.5",
        "twbs/bootstrap": "~3.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^5.7"
    },
    "suggest": {
        "integrated/channel-bundle": "Manage channel configuration"
    },
    "autoload":{
        "psr-4":{
            "Integrated\\Bundle\\WebsiteBundle\\": ""
        }
    }
}
