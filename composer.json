{
    "name": "integrated/page-bundle",
    "type": "symfony-bundle",
    "description": "Provides the possibility for the user to create pages on Integrated websites, and front-end editing when combined with the website-bundle",
    "keywords": ["Integrated", "page"],
    "homepage": "http://www.integratedfordevelopers.com",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "http://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.8",
        "sensio/framework-extra-bundle": "~3.0",
        "doctrine/mongodb": "~1.3",
        "doctrine/mongodb-odm": "~1.1.2",
        "doctrine/mongodb-odm-bundle": "~3.2",
        "integrated/content-bundle": "~0.6",
        "integrated/formtype-bundle": "~0.6",
        "integrated/block-bundle": "~0.6",
        "integrated/theme-bundle": "~0.6",
        "knplabs/knp-paginator-bundle": ">=2.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*",
        "phpunit/phpunit": "~4.8"
    },
    "suggest": {
        "integrated/website-bundle": "Frontend block management for Integrated"
    },
    "autoload":{
        "psr-0":{
            "Integrated\\Bundle\\PageBundle": ""
        }
    },
    "target-dir": "Integrated/Bundle/PageBundle"
}
