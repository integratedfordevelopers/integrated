{
    "name": "integrated/integrated",
    "type": "library",
    "description": "Integrated PHP framework",
    "keywords": [
        "Integrated",
        "content store",
        "publishing"
    ],
    "homepage": "https://www.integratedfordevelopers.com",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "https://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "symfony/symfony": "~2.8 || ~3.0",
        "symfony/security-acl": "~2.8 || ~3.0",
        "doctrine/orm": "~2.5",
        "doctrine/mongodb": "~1.3",
        "doctrine/mongodb-odm": "~1.1",
        "solarium/solarium": "~3.7",
        "psr/log": "~1.0"
    },
    "replace": {
        "integrated/library": "self.version"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Integrated\\MongoDB\\": "src/MongoDB",
            "Integrated\\Doctrine\\": "src/Doctrine",
            "Integrated\\Common\\": "src/Common"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Integrated\\Tests\\MongoDB\\": "tests/MongoDB",
            "Integrated\\Tests\\Doctrine\\": "tests/Doctrine",
            "Integrated\\Tests\\Common\\": "tests/Common"
        }
    }
}
