{
    "name": "integrated/content-bundle",
    "type": "symfony-bundle",
    "description": "Integrated Content Bundle",
    "keywords": ["Integrated", "content"],
    "homepage": "http://www.e-active.nl",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "http://www.integratedfordevelopers.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "ext-mongo": "~1.2",
        "symfony/symfony": "~2.6",
        "symfony/framework-bundle": "~2.2",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/mongodb": ">=1.0.0",
        "doctrine/mongodb-odm": "*",
        "doctrine/mongodb-odm-bundle": "~3.0",
        "integrated/library": "~0.3",
        "integrated/formtype-bundle": "~0.2",
        "integrated/locking-bundle": "~0.2",
        "integrated/menu-bundle": "~0.3",
        "integrated/block-bundle": "~0.2",
        "integrated/slug-bundle": "~0.2",
        "knplabs/knp-menu-bundle": "~2.0",
        "knplabs/knp-paginator-bundle": "~2.4",
        "nelmio/alice": "1.*",
        "braincrafted/bootstrap-bundle": "~2.0",
        "twbs/bootstrap": "3.*",
        "friendsofsymfony/jsrouting-bundle": "~1.5",
        "vihuvac/recaptcha-bundle": "dev-master"
    },
    "suggest": {
        "integrated/page-bundle": "~0.2",
        "integrated/website-bundle": "~0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*"
    },
    "autoload":{
        "psr-0":{
            "Integrated\\Bundle\\ContentBundle": ""
        }
    },
    "target-dir": "Integrated/Bundle/ContentBundle",
    "extra": {
        "branch-alias": {
            "dev-develop": "0.9.x-dev"
        }
    }
}
