{
    "name": "integrated/integrated",
    "type": "library",
    "description": "Integrated PHP framework",
    "keywords": [
        "integrated",
        "content store",
        "publishing"
    ],
    "homepage": "https://www.integratedfordevelopers.com",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "https://www.e-active.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-intl": "*",
        "ext-json": "*",
        "abraham/twitteroauth": "^0.7.4",
        "darsyn/ip": "^4.0",
        "doctrine/cache": "^1.11",
        "doctrine/data-fixtures": ">= 1.5.1",
        "doctrine/doctrine-bundle": "^2.4.3",
        "doctrine/doctrine-fixtures-bundle": "^3.4.0",
        "doctrine/migrations": "~2.2",
        "doctrine/orm": "^2.7.5",
        "endroid/qr-code-bundle": "^3.4",
        "facebook/graph-sdk": "~5.0",
        "gregwar/image": ">=2.0.25",
        "gregwar/image-bundle": "~2.1",
        "jms/metadata": "~1.6",
        "knplabs/knp-gaufrette-bundle": "~0.5",
        "knplabs/knp-menu-bundle": "~2.2",
        "knplabs/knp-paginator-bundle": "~5.0",
        "monolog/monolog": "~1.22",
        "nelmio/alice": "~3.1",
        "rct567/dom-query": "~0.8",
        "scheb/two-factor-bundle": "^4.18",
        "sensio/framework-extra-bundle": "~5.0",
        "solarium/solarium": "~5.2",
        "swiftmailer/swiftmailer": "~6.1",
        "symfony/asset": "~4.4",
        "symfony/cache": "~4.4",
        "symfony/dom-crawler": "~4.4",
        "symfony/form": "~4.4",
        "symfony/http-foundation": "~4.4",
        "symfony/intl": "~4.4",
        "symfony/lock": "~4.4",
        "symfony/mailer": "~4.4",
        "symfony/monolog-bundle": "~3.7.1",
        "symfony/polyfill-iconv": "~1.6",
        "symfony/polyfill-php80": "^1.23.1",
        "symfony/routing": "~4.4",
        "symfony/security-acl": "~3.0",
        "symfony/security-bundle": "~4.4.1",
        "symfony/serializer": "~4.4",
        "symfony/translation": "~4.4",
        "symfony/twig-bundle": "~4.4",
        "symfony/validator": "~4.4",
        "twig/twig": ">=2.12",
        "doctrine/mongodb-odm-bundle": "~4.3",
        "psr/simple-cache": "^1.0",
        "doesntmattr/mongodb-migrations-bundle": "^3.0",
        "excelwebzone/recaptcha-bundle": "^1.5"
    },
    "replace": {
        "integrated/asset-bundle": "self.version",
        "integrated/block-bundle": "self.version",
        "integrated/channel-bundle": "self.version",
        "integrated/comment-bundle": "self.version",
        "integrated/content-bundle": "self.version",
        "integrated/content-history-bundle": "self.version",
        "integrated/formtype-bundle": "self.version",
        "integrated/image-bundle": "self.version",
        "integrated/library": "self.version",
        "integrated/locking-bundle": "self.version",
        "integrated/menu-bundle": "self.version",
        "integrated/page-bundle": "self.version",
        "integrated/slug-bundle": "self.version",
        "integrated/social-bundle": "self.version",
        "integrated/solr-bundle": "self.version",
        "integrated/storage-bundle": "self.version",
        "integrated/theme-bundle": "self.version",
        "integrated/user-bundle": "self.version",
        "integrated/website-bundle": "self.version",
        "integrated/workflow-bundle": "self.version"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.1",
        "phpunit/phpunit": "~8.5",
        "friendsofphp/php-cs-fixer": "^2.8",
        "phpstan/phpstan": "^0.12.99",
        "phpstan/phpstan-doctrine": "^0.12.44",
        "phpstan/phpstan-deprecation-rules": "^0.12.6"
    },
    "provide": {
        "ext-mongo": "*"
    },
    "autoload": {
        "psr-4": {
            "Integrated\\Bundle\\": "src/Bundle",
            "Integrated\\MongoDB\\": "src/MongoDB",
            "Integrated\\Doctrine\\": "src/Doctrine",
            "Integrated\\Common\\": "src/Common"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-release/0.10": "0.10.x-dev",
            "dev-release/0.11": "0.11.x-dev",
            "dev-release/0.12": "0.12.x-dev",
            "dev-release/0.13": "0.13.x-dev",
            "dev-release/0.14": "0.14.x-dev",
            "dev-release/0.15": "0.15.x-dev",
            "dev-release/0.20": "0.20.x-dev"
        }
    }
}
