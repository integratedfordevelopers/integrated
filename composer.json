{
    "name": "integrated/slug-bundle",
    "type": "symfony-bundle",
    "description": "Provides a slugger which can generate a slug from a string and event listeners to auto-generate slugs on chosen fields",
    "keywords": ["Integrated", "slug"],
    "homepage": "http://www.integratedfordevelopers.com",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "http://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "lib-iconv": "*",
        "symfony/symfony": ">=2.4",
        "symfony/framework-bundle": ">=2.2",
        "jms/metadata": "1.5.*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*",
        "phpunit/phpunit": "~4.8"
    },
    "autoload":{
        "psr-0":{
            "Integrated\\Bundle\\SlugBundle": ""
        }
    },
    "target-dir": "Integrated/Bundle/SlugBundle"
}
