{
    "name": "integrated/locking-bundle",
    "type": "symfony-bundle",
    "description": "Provides an interface to manage database locks and keep a lock active by Ajax request",
    "keywords": ["Integrated", "locking"],
    "homepage": "http://www.integratedfordevelopers.com",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "http://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.4",
        "symfony/framework-bundle": "~2.2",
        "integrated/library": "~0.1"
    },
    "require-dev": {
        "doctrine/dbal": ">=2.3.0,<2.5-dev",
        "squizlabs/php_codesniffer": "2.*",
        "phpunit/phpunit": "~4.8"
    },
    "autoload":{
        "psr-0":{
            "Integrated\\Bundle\\LockingBundle": ""
        }
    },
    "target-dir": "Integrated/Bundle/LockingBundle"
}