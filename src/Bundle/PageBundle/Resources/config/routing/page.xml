<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="integrated_page_page_index" path="/" methods="GET">
        <default key="_controller">IntegratedPageBundle:Page:index</default>
    </route>

    <route id="integrated_page_page_new" path="/new" methods="GET|POST">
        <default key="_controller">IntegratedPageBundle:Page:new</default>
    </route>

    <route id="integrated_page_page_edit" path="/{id}/edit" methods="GET|PUT">
        <default key="_controller">IntegratedPageBundle:Page:edit</default>
    </route>

    <route id="integrated_page_page_delete" path="/{id}/delete" methods="GET|DELETE">
        <default key="_controller">IntegratedPageBundle:Page:delete</default>
    </route>

    <route id="integrated_page_page_copy" path="/copy" methods="GET|POST">
        <default key="_controller">IntegratedPageBundle:Page:copy</default>
    </route>
</routes>
