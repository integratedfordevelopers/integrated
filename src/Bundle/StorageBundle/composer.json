{
    "name": "integrated/storage-bundle",
    "type": "symfony-bundle",
    "description": "Integrated Storage Bundle",
    "keywords": ["Integrated", "Storage"],
    "homepage": "http://www.e-active.nl",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "http://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.8 || ~3.0",
        "integrated/library": "~0.7",
        "integrated/asset-bundle": "~0.7",
        "integrated/content-bundle": "~0.7",
        "integrated/image-bundle": "~0.7",
        "knplabs/knp-gaufrette-bundle": "~0.1.7",
        "doctrine/collections": "~1.3",
        "fzaninotto/faker": "~1.6"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^5.7"
    },
    "suggest": {
        "ext-imagick": "Allows more image formats to be uploaded"
    },
    "autoload":{
        "psr-4":{
            "Integrated\\Bundle\\StorageBundle\\": ""
        }
    }
}
