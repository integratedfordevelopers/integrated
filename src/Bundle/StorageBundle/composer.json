{
    "name": "integrated/storage-bundle",
    "type": "symfony-bundle",
    "description": "Integrated Storage Bundle",
    "keywords": [
        "integrated",
        "storage"
    ],
    "homepage": "https://www.integratedfordevelopers.com",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "https://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "~2.8 || ~3.0",
        "doctrine/collections": "~1.3",
        "integrated/library": "~0.8",
        "integrated/asset-bundle": "~0.8",
        "integrated/content-bundle": "~0.8",
        "integrated/image-bundle": "~0.8",
        "knplabs/knp-gaufrette-bundle": "~0.4",
        "fzaninotto/faker": "~1.7"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^5.7"
    },
    "suggest": {
        "ext-imagick": "Allows more image formats to be uploaded"
    },
    "autoload": {
        "psr-4": {
            "Integrated\\Bundle\\StorageBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
