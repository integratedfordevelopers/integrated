{
    "name": "integrated/channel-bundle",
    "type": "symfony-bundle",
    "description": "Provides the possibility to use and configure connectors on Integrated installation, which get or distribute content",
    "keywords": [
        "integrated",
        "channel",
        "multi-site"
    ],
    "homepage": "https://www.integratedfordevelopers.com",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "https://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "~2.8 || ~3.0",
        "alcaeus/mongo-php-adapter": "~1.1",
        "doctrine/doctrine-bundle": "~1.8",
        "doctrine/mongodb-odm-bundle": "~3.4",
        "integrated/library": "~0.8",
        "braincrafted/bootstrap-bundle": "~2.2",
        "knplabs/knp-paginator-bundle": "~2.6"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^5.7"
    },
    "provide": {
        "ext-mongo": "*"
    },
    "autoload": {
        "psr-4": {
            "Integrated\\Bundle\\ChannelBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
