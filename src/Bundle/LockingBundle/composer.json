{
    "name": "integrated/locking-bundle",
    "type": "symfony-bundle",
    "description": "Provides an interface to manage database locks and keep a lock active by Ajax request",
    "keywords": [
        "integrated",
        "locking"
    ],
    "homepage": "https://www.integratedfordevelopers.com",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "https://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "~2.8 || ~3.0",
        "integrated/library": "~0.8"
    },
    "require-dev": {
        "doctrine/dbal": "~2.5",
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Integrated\\Bundle\\LockingBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
