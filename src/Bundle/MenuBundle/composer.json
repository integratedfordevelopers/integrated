{
    "name": "integrated/menu-bundle",
    "type": "symfony-bundle",
    "description": "Provides MenuEvents to allow adding navigation menu items to the Integrated backend and a database provider to read navigation items from the database (which allows front-end navigation menu editing when used with the website-bundle)",
    "keywords": [
        "integrated",
        "menu"
    ],
    "homepage": "https://www.integratedfordevelopers.com",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "https://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "~2.8 || ~3.0",
        "integrated/library": "~0.8",
        "integrated/content-bundle": "~0.8",
        "knplabs/knp-menu-bundle": "~2.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Integrated\\Bundle\\MenuBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
