{
    "name": "integrated/content-bundle",
    "type": "symfony-bundle",
    "description": "Integrated Content Bundle",
    "keywords": [
        "integrated",
        "content"
    ],
    "homepage": "https://www.integratedfordevelopers.com",
    "license": "MIT",
    "authors": [
        {
            "name": "e-Active",
            "homepage": "https://www.e-active.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "ext-intl": "*",
        "symfony/symfony": "~2.8 || ~3.0",
        "alcaeus/mongo-php-adapter": "~1.1",
        "doctrine/doctrine-fixtures-bundle": "~2.4",
        "doctrine/mongodb-odm-bundle": "~3.4",
        "integrated/library": "~0.8",
        "integrated/formtype-bundle": "~0.8",
        "integrated/locking-bundle": "~0.8",
        "integrated/menu-bundle": "~0.8",
        "integrated/block-bundle": "~0.8",
        "integrated/slug-bundle": "~0.8",
        "integrated/solr-bundle": "~0.8",
        "integrated/image-bundle": "~0.8",
        "integrated/asset-bundle": "~0.8",
        "integrated/workflow-bundle": "~0.8",
        "braincrafted/bootstrap-bundle": "~2.2",
        "gregwar/image-bundle": "~2.1",
        "knplabs/knp-menu-bundle": "~2.1",
        "knplabs/knp-paginator-bundle": "~2.6",
        "nelmio/alice": "~3.1",
        "twbs/bootstrap": "~3.3",
        "vihuvac/recaptcha-bundle": "~2.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^5.7"
    },
    "provide": {
        "ext-mongo": "*"
    },
    "suggest": {
        "integrated/page-bundle": "~0.5",
        "integrated/website-bundle": "~0.5"
    },
    "autoload": {
        "psr-4": {
            "Integrated\\Bundle\\ContentBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    }
}
