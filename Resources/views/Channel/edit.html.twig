{% extends 'IntegratedContentBundle::base.html.twig' %}
{% block content %}
    <div class="content_columns_holder">
        <div class="content_column_inner">
            <ol class="breadcrumb">
                <li>
                    <a href="{{ path('integrated_content_channel_index') }}">{% trans %}Home{% endtrans %}</a>
                </li>
                <li class="active">{% trans %}Edit{% endtrans %}</li>
            </ol>
            <div class="page-header">
                <h1>{% trans %}Edit{% endtrans %}</h1>
            </div>
            {{ form(form, {'style': 'horizontal'}) }}
        </div>
    </div>
{% endblock %}

{% block javascript %}
    {{ parent() }}
    {% javascripts "@IntegratedContentBundle/Resources/public/js/channel.js" %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock javascript %}