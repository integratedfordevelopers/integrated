<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
		   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		   xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

	<services>

		<service id="integrated_content.content.relations.type" class="%integrated_content.form.proxy.type.class%">
			<argument type="service" id="integrated_content.form.content.relations.type" />
			<argument type="string">content_relations</argument>

			<tag name="form.type" alias="content_relations" />
		</service>

		<service id="integrated_content.content.delete.form.type" class="%integrated_content.form.proxy.type.class%">
			<argument type="service" id="integrated_content.form.delete.type" />
			<argument type="string">content_delete</argument>

			<tag name="form.type" alias="content_delete" />
		</service>

	</services>

</container>