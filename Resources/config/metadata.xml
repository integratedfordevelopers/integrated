<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>

        <parameter key="integrated_website.metadata.factory.type.class">Integrated\Common\Block\BlockInterface</parameter>

    </parameters>

    <services>

        <service id="integrated_website.metadata.factory" class="%integrated_content.metadata.factory.class%">
            <argument type="service" id="integrated_content.metadata.driver" />
            <argument>%integrated_website.metadata.factory.type.class%</argument>

            <call method="setEventDispatcher">
                <argument type="service" id="integrated_content.metadata.factory.event_dispatcher" />
            </call>
        </service>

    </services>

</container>