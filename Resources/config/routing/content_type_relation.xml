<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="integrated_content_content_type_relation_index" path="/" methods="GET">
        <default key="_controller">IntegratedContentBundle:ContentTypeRelation:index</default>
    </route>

    <route id="integrated_content_content_type_relation_new" path="/new" methods="GET">
        <default key="_controller">IntegratedContentBundle:ContentTypeRelation:new</default>
    </route>

    <route id="integrated_content_content_type_relation_create" path="/" methods="POST">
        <default key="_controller">IntegratedContentBundle:ContentTypeRelation:create</default>
    </route>

    <route id="integrated_content_content_type_relation_edit" path="/{id}/edit" methods="GET">
        <default key="_controller">IntegratedContentBundle:ContentTypeRelation:edit</default>
    </route>

    <route id="integrated_content_content_type_relation_update" path="/{id}" methods="PUT">
        <default key="_controller">IntegratedContentBundle:ContentTypeRelation:update</default>
    </route>

    <route id="integrated_content_content_type_relation_delete" path="/{id}" methods="DELETE">
        <default key="_controller">IntegratedContentBundle:ContentTypeRelation:delete</default>
    </route>

    <route id="integrated_content_content_type_relation_show" path="/{id}" methods="GET">
        <default key="_controller">IntegratedContentBundle:ContentTypeRelation:show</default>
    </route>
</routes>