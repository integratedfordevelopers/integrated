<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
		   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		   xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

	<services>

		<service id="integrated_user.group.choice.type" class="%integrated_user.form.proxy.type.class%">
			<argument type="service" id="integrated_user.form.group.choice.type" />
			<argument type="string">user_group_choice</argument>

			<tag name="form.type" alias="user_group_choice" />
		</service>

		<service id="integrated_user.group.edit.form.type" class="%integrated_user.form.proxy.type.class%">
			<argument type="service" id="integrated_user.form.group.type" />
			<argument type="string">user_group_new</argument>

			<tag name="form.type" alias="user_group_new" />
		</service>

		<service id="integrated_user.group.new.form.type" class="%integrated_user.form.proxy.type.class%">
			<argument type="service" id="integrated_user.form.group.type" />
			<argument type="string">user_group_edit</argument>

			<tag name="form.type" alias="user_group_edit" />
		</service>

		<service id="integrated_user.group.delete.form.type" class="%integrated_user.form.proxy.type.class%">
			<argument type="service" id="integrated_user.form.delete.type" />
			<argument type="string">user_group_delete</argument>

			<tag name="form.type" alias="user_group_delete" />
		</service>

	</services>

</container>